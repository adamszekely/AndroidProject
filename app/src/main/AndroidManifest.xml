        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register.Register" />
        <activity android:name=".Login.Login" />
        <activity android:name=".VehicleDetails.VehicleDetails" />
        <activity android:name=".Favourites.Favourites" />
        <activity android:name=".Search.Search" />
        <activity android:name=".Loading" />
        <activity android:name=".Search.AddBrand" />
        <activity android:name=".Search.AddModel"></activity>
    </application>

</manifest>